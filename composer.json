{
    "name": "pierresilva/laravel-jwt-auth",
    "description": "Laravel JWT auth api",
    "keywords": ["laravel", "JWT", "Authentication"],
    "type": "package",
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": ">=5.8",
        "laravel/framework": ">=5.8.0",
        "tymon/jwt-auth": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Silva",
            "email": "pierremichelsilva@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "pierresilva\\JWTAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "pierresilva\\JWTAuth\\JWTAuthServiceProvider"
            ]
        }
    }
}
